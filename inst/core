    { 51, 2, OR, "OR" },
    { 52, 2, XOR, "XOR" },
    { 69, 2, JUMPF, "JUMPF" },
    { 78, 1, MCOPY, "MCOPY" },
    { 79, 1, MXCHG, "MXCHG" },
    { 81, 2, XCHG, "XCHG" },
    { 84, 2, IN, "IN" },
    { 85, 2, OUT, "OUT" },
