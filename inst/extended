local function LIDTR(vm) 
    vm.IDTR = op1;
end

local function EXTRET(vm) 
    
end

local function STEF(vm) 
    vm.extended_flag = 1;
end

local function CLEF(vm) 
    vm.extended_flag = 0;
end

local function IRET(vm) 
    local cs = vm:Pop()
    if(vm.interrupt_flag ~= 0) then return end

    local ip = vm:Pop();
    if(vm.interrupt_flag ~= 0) then return end
    
    vm:JMP(ip, cs);
end

local function EXTINT(vm) 
    
end

local function STD2(vm) 
    
end

return {
    { 41, 0, IRET, "IRET" },
    { 48, 0, STEF, "STEF" },
    { 49, 0, CLEF, "CLEF" },
    { 70, 0, EXTINT, "EXTINT" },
    { 99, 1, LIDTR, "LIDTR" },
    { 110, 0, EXTRET, "EXTRET" },
    { 116, 0, STD2, "STD2" },
}